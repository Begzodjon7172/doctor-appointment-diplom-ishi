<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/doctor_graph"
    app:startDestination="@id/doctorHomeFragment">


    <fragment
        android:id="@+id/doctorHomeFragment"
        android:name="uz.bnabiyev.drappointment.fragments.DoctorHomeFragment"
        android:label="fragment_doctor_home"
        tools:layout="@layout/fragment_doctor_home">
        <action
            android:id="@+id/action_doctorHomeFragment_to_sendMessageFragment2"
            app:destination="@id/sendMessageFragment2" />
        <action
            android:id="@+id/action_doctorHomeFragment_to_doctorChatFragment"
            app:destination="@id/doctorChatFragment" />
        <action
            android:id="@+id/action_doctorHomeFragment_to_doctorAppointmentListFragment"
            app:destination="@id/doctorAppointmentListFragment" />
    </fragment>
    <fragment
        android:id="@+id/sendMessageFragment2"
        android:name="uz.bnabiyev.drappointment.fragments.SendMessageFragment"
        android:label="fragment_send_message"
        tools:layout="@layout/fragment_send_message" />
    <fragment
        android:id="@+id/doctorAppointmentListFragment"
        android:name="uz.bnabiyev.drappointment.fragments.DoctorAppointmentListFragment"
        android:label="fragment_doctor_appointment_list"
        tools:layout="@layout/fragment_doctor_appointment_list">
    </fragment>
    <fragment
        android:id="@+id/doctorChatFragment"
        android:name="uz.bnabiyev.drappointment.fragments.DoctorChatFragment"
        android:label="fragment_doctor_chat"
        tools:layout="@layout/fragment_doctor_chat" />

</navigation>